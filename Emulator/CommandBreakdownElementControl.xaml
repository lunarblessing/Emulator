<UserControl x:Class="EmulatorUI.CommandBreakdownElementControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EmulatorUI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Margin="10">
    <Border BorderThickness="1,0,1,0" BorderBrush="Black" Padding="5,0,5,0">
        <StackPanel>
            <TextBlock x:Name="BitsTBlock" Margin="1,5,1,5"/>
            <TextBlock x:Name="DescTBlock" Margin="1,5,1,5"/>
        </StackPanel>
    </Border>

</UserControl>
